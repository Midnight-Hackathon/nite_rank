{
  "name": "nightrunner",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "docker": "docker run -p 6300:6300 midnightnetwork/proof-server -- 'midnight-proof-server --network testnet'",
    "compile": "compact compile contracts/aseryx.compact contracts/managed/aseryx",
    "build": "tsc",
    "build:frontend": "vite build",
    "dev": "vite",
    "clean": "rm -rf dist && rm -rf contracts/managed/",
    "rebuild": "npm run clean && npm run build",
    "deploy": "node --disable-warning=ExperimentalWarning --disable-warning=DEP0040 dist/scripts/deploy.js",
    "create:wallet": "node --disable-warning=ExperimentalWarning --disable-warning=DEP0040 dist/scripts/createWallet.js",
    "register": "node --disable-warning=ExperimentalWarning --disable-warning=DEP0040 dist/scripts/register.js",
    "submit:proof": "node --disable-warning=ExperimentalWarning --disable-warning=DEP0040 dist/scripts/submitProof.js",
    "verifyProof:onchain": "node --disable-warning=ExperimentalWarning --disable-warning=DEP0040 dist/zk/verifyProofOnChain.js",
    "verifyProof:offchain": "node --disable-warning=ExperimentalWarning --disable-warning=DEP0040 dist/zk/verifyProofOffChain.js",
    "fetch:tx": "node --disable-warning=ExperimentalWarning --disable-warning=DEP0040 dist/utils/fetchTransaction.js",
    "backend": "node backend/server.js",
    "server": "cd server && node submitProof.js"
  },
  "dependencies": {
    "@midnight-ntwrk/compact-runtime": "^0.9.0",
    "@midnight-ntwrk/ledger": "^4.0.0",
    "@midnight-ntwrk/midnight-js-contracts": "2.0.2",
    "@midnight-ntwrk/midnight-js-http-client-proof-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-indexer-public-data-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-level-private-state-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-network-id": "2.0.2",
    "@midnight-ntwrk/midnight-js-node-zk-config-provider": "2.0.2",
    "@midnight-ntwrk/midnight-js-types": "2.0.2",
    "@midnight-ntwrk/wallet": "5.0.0",
    "@midnight-ntwrk/wallet-api": "5.0.0",
    "@midnight-ntwrk/zswap": "^4.0.0",
    "bech32": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "lucide-react": "^0.554.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^24.4.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^5.0.0",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0"
  }
}
